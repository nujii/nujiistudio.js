{namespace org.plovr}

/**
 * @param title
 * @param baseJsUrl
 * @param testJsUrl
 */
{template .test}
<!doctype html>
<html>
<head>
  <title>{$title} test</title>
</head>
<body>
  <script src="{$baseJsUrl}"></script>
  <script> 
  	goog.require('goog.testing.AsyncTestCase');
  	goog.require('goog.testing.jsunit');
  </script>
  <script src="{$testJsUrl}"></script>
</body>
</html>
{/template}